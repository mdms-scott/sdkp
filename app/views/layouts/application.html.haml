!!!
%html
  %head
    %title #{title}
    = csrf_meta_tag
    = stylesheet_link_tag :all
    = javascript_include_tag :all
  %body
    #binder
      = yield
      = debug(params) if Rails.env.development?
    = render :partial => 'layouts/footer'
    - unless User.all.empty?
      - if current_user
        = render :partial => 'layouts/panel'
